# Production Environment Variables
# Copy this file to .env on your production server

# App
PORT=3007
NODE_ENV=production

# Database (will be created in ./data folder)
DATABASE_URL="file:./obi.db"

# Fonnte WhatsApp API
FONNTE_API_KEY=your_fonnte_api_key_here

# AI Proxy (proxy.kelazz.my.id)
AI_PROXY_KEY=your_ai_proxy_key_here
AI_MODEL=gemini-2.5-flash

# Default recipient phone number (with country code, no +)
DEFAULT_PHONE=6281234567890

# Authentication
JWT_SECRET=please_generate_a_random_secret_string
ADMIN_PASSWORD=your_secure_password_here
